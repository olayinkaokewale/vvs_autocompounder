    uint256 amountOut, // 10000000000000000000 (10 CRO)
    uint256 amountInMax,  // 45836720352479696121718 (Eq. VVS)
    address[] path, // [ 0x2d03bece6747adc00e1a131bba1469c15fd11e03 (VVS), 0x5c7f8a570d578ed84e63fdfa7b1ee72deae1ae23 (WCRO) ]
    address to, // 0x82f25c3343e8ab407319de8dfe20f9925cecb8eb (My Address)
    uint256 deadline // 1638750962 - UNIX time in seconds
)

2. Swap VVS for USDC (for LP)
