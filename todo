)

3. Deposit into VVS-USDC Pool
contract interacted with: 0x145863eb42cf62847a6ca784e6416c1682b1b2ae
addLiquidity (
    address tokenA, // 0x2d03bece6747adc00e1a131bba1469c15fd11e03 (VVS)
    address tokenB, // 0xc21223249ca28397b4b6541dffaecc539bff0c59 (USDC)
    uint256 amountADesired, // 88572416100167252617661 (VVS Amt. - 10^18)
    uint256 amountBDesired, // 10927154 (USDC Amt. - 10^6)
    uint256 amountAMin, // 88129554019666416354572 (Amount desired - 443 VVS )
    uint256 amountBMin, // 10872518 (Amount desired - 0.5 USDC)
    address to, // 0x82f25c3343e8ab407319de8dfe20f9925cecb8eb (My Address)
    uint256 deadline //1638751042 - UNIX time in seconds
)

